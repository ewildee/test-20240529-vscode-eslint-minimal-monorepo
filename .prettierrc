{
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 90,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "plugins": ["prettier-plugin-svelte"],
        "htmlWhitespaceSensitivity": "css"
      }
    }
  ]
}
